<footer id="footer">
  <% if (theme.sidebar === 'bottom'){ %>
    <%- partial('_partial/sidebar') %>
  <% } %>
  <div class="outer">
    <div id="footer-info" class="inner">
      &copy; <%= date(new Date(), 'YYYY') %> <i class="fa fa-<%- theme.authoricon %>"></i>
  	  <%= config.author || config.title %><br>
      <a href="http://hexo.io/" target="_blank">Hexo</a> <%= __('powered_by') %>
    </div>
  </div>
</footer>